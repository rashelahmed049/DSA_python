
class node:
    def __init__(self,data):
        self.d = data
        self.next = None
        self.prev = None
class LL:
    def __init__(self):
        self.head = None
    def insert_at_beg(self,data):
        nw = node(data)
        self.head = nw
    def insert_at_end(self,data):
        if self.head is None:
            nw = node(data)
            self.head = nw
        else:
            nw = node(data)
            t = self.head
            while t.next:
                t = t.next
            t.next = nw
            nw.prev = t
    def dis(self):
        c = self.head
        while c:
            print(c.d)
            c = c.next
l = LL()
l.insert_at_beg(3)
l.insert_at_end(7)
l.dis()
            
