class node:
    def __init__(self,data):
        self.d = data
        self.next = None
        self.prev = None
class dl:
    def __init__(self):
        self.head = None
    def ib(self,data):
        new_node = node(data)
        if self.head is None:
            self.head = new_node
        else:
            new_node.next = self.head
            self.head.prev = new_node
            self.head = new_node
    def i_af(self,key,data):
        temp = self.head
        while temp and temp.d != key:
            temp = temp.next
        if temp is None:
            print("Value not found")
            return
        new_node = node(data)
        new_node.next = temp.next
        new_node.prev = temp
        if temp.next is not None:
            temp.next.prev = new_node

        temp.next = new_node
                    
    def del_b(self):
        if self.head is None:
            print("Empty list! Cannot delete..")
        elif self.head.next is None:
            self.head = None
        else:
            self.head = self.head.next
            self.head.prev = None
            
    def del_d(self,data):
        t = self.head
        if t.d == data:
            self.head == self.head.next
            self.head.prev = None
        else:
            while t:
                t = t.next
                if t.d == data:
                    t.prev.next = t.next
                    t.next.prev = t.prev
                    break
            
    def disp(self):
        t = self.head
        while t:
            print(t.d,end = " ")
            t = t.next
        print()
db = dl()
db.ib(9)
db.ib(8)
db.ib(20)
db.disp()
db.del_b()
db.disp()
db.ib(18)
db.disp()
db.del_d(8)
db.disp()
db.i_af(18,6)
db.disp()
            
            
