from collections import deque

class BinaryTree:
    def __init__(self):
        self.root = None

    def insert(self, data):
        new_node = Node(data)

        if self.root is None:
            self.root = new_node
            return

        q = deque([self.root])

        while q:
            temp = q.popleft()

            if temp.left is None:
                temp.left = new_node
                return
            else:
                q.append(temp.left)

            if temp.right is None:
                temp.right = new_node
                return
            else:
                q.append(temp.right)
